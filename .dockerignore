# Ignore the default SQLite database.
db/*.sqlite3
db/*.sqlite3-journal
db/*.sqlite3-*

# Ignore all logfiles and tempfiles.
log/*
tmp/*
!log/.keep
!tmp/.keep

# Ignore pidfiles, but keep the directory.
tmp/pids/*
!tmp/pids/
!tmp/pids/.keep

# Ignore uploaded files in development.
storage/*
!storage/.keep

.byebug_history

public/packs-test
node_modules
yarn-error.log
yarn-debug.log*
.yarn-integrity

# Ignore local folder
local/*
!local/.keep

# Ignore kubernetes config files
kube
